@model IEnumerable<bstLesson06.Models.bstSong>

@{
    ViewBag.Title = "bst: Danh sách bài hát ";
}

<h2>bst: Danh sách bài hát</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.bstTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.bstAuthor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.bstArtist)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.bstYearRelease)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.bstTitle)
        </td>
